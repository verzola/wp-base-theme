    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.2",
